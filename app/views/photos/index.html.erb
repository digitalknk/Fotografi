<% @photos.each do |photo| %>

<div class="photo-group thumbnail">
      <div class="photo photo-wrapper">
      <span class="photo-title">Photo Title here soon!</span>
      <%= image_tag photo.image_url, :class => 'img-rounded'%>
      <div class="social-links">
        <div class="g-plusone" data-size="medium" data-href="http://urltogp.com"></div>
        <br />
        <a href="https://twitter.com/share" class="twitter-share-button" data-url="http://www.example.com" data-text="PhotoTitle" data-via="TwitterUserName" data-hashtags="photo">Tweet</a>
        <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script>
        <br />
        <div class="fb-like" data-href="http://example.com" data-send="false" data-layout="button_count" data-width="107" data-show-faces="false"></div>
      </div>
      <span class="taken-by">Photo taken by <%= photo.user.full_name %></span>
    <% if current_user %>
      <span class="admin-links">
        <i class="icon-edit icon-white"></i> <%= link_to 'Edit', edit_photo_path(photo) %> | 
        <i class="icon-remove icon-white"></i> <%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %>
      </span>
    <% end %>
  </div>
</div>
  <br />
<% end %>

<br />
